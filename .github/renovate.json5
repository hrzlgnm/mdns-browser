{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automergeType": "pr",
  "patch": {
    "automerge": true
  },
  "minor": {
    "automerge": true
  },
  "pinDigests": true,
  "extends": [
    "config:recommended",
    ":dependencyDashboard",
    ":enableVulnerabilityAlerts",
    ":rebaseStalePrs",
    ":semanticCommits",
    ":semanticPrefixChore",
    ":separateMajorReleases",
    ":prImmediately"
  ],
  "prConcurrentLimit": 0,
  "lockFileMaintenance": { "enabled": true }
}
